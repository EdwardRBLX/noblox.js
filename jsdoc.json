{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": "lib",
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown",
        "plugins/categories"
    ],
    "opts": {
        "template": "node_modules/minami",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "useLongnameInNav": false,
        "showInheritedInNav": true
    }
}